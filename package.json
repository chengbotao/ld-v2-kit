{
  "name": "ld-v2-kit",
  "version": "1.0.1",
  "description": "基于 Vue2 + ElementUI 的轻量级工具组件包，包含自定义 UI 组件、业务组件与常用工具类。",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/chengbotao/ld-v2-kit.git"
  },
  "keywords": [
    "ld-v2-kit",
    "Vue2",
    "vue2.6",
    "ElementUI",
    "element-ui",
    "component",
    "components",
    "utils",
    "toolkit",
    "vue-component",
    "业务组件",
    "轻量组件库"
  ],
  "author": "chengbotao (慢知行 LearnDoing)",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0 <15.0.0",
    "npm": ">=6.0.0"
  },
  "sideEffects": false,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "peerDependencies": {
    "element-ui": "~2.15.14",
    "vue": "~2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.6",
    "@babel/runtime": "^7.28.6",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "core-js": "^3.47.0",
    "element-ui": "~2.15.14",
    "rollup": "^3.29.5",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "volta": {
    "node": "14.21.3"
  }
}
